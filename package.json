{
  "name": "biirus.github.io",
  "private": true,
  "version": "0.0.1",
  "description": "Ivan Burnaev's personal website",
  "scripts": {
    "build:client": "NODE_ENV=production stencil build --prerender",
    "build": "bash ./scripts/build.sh",
    "dev:client": "stencil build --dev --watch --serve",
    "dev:server": "cd ./server && npm run dev",
    "dev": "NODE_ENV=development npm-run-all --parallel dev:*",
    "predeploy": "npm run build && git add . && git commit -m 'deploy'",
    "deploy": "git subtree push --prefix server heroku master",
    "test:client": "stencil test --spec --e2e",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "test": "exit 0",
    "generate": "stencil generate",
    "postinstall": "cd ./server && npm install"
  },
  "devDependencies": {
    "@stencil/core": "^1.12.4",
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/router": "^1.0.1",
    "@types/node": "^13.13.4",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "npm-run-all": "^4.1.5",
    "rollup-plugin-dotenv": "^0.2.0"
  },
  "license": "MIT",
  "dependencies": {
    "@stencil/helmet": "^0.3.1"
  }
}
